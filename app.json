{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "frontend",
    "newArchEnabled": true,

    "ios": {
      "bundleIdentifier": "com.yourcompany.walletwave",
      "supportsTablet": true
    },

    "android": {
      "package": "com.yourcompany.walletwave",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png"
      }
    },

    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-web-browser",

      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-8525673711213815~4747102206",
          "iosAppId": "ca-app-pub-8525673711213815~6303726453"
        }
      ]
    ],

    "extra": {
      "eas": {
        "projectId": "8b7d9fad-748d-43c0-b7c3-6f94014b35b4"
      }
    }
  }
}
