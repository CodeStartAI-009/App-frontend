{
  "expo": {
    "name": "UniSpend",
    "slug": "frontend",
    "version": "1.0.3",
    "orientation": "portrait",

    "icon": "./assets/images/image.png",
    "scheme": "frontend",

    "jsEngine": "hermes",
    "newArchEnabled": true,

    "ios": {
      "bundleIdentifier": "com.codestart.unispend",
      "supportsTablet": true
    },

    "android": {
      "package": "com.codestart.unispend",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/image.png",
        "backgroundColor": "#ffffff"
      }
    },

    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/image.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-web-browser",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-8525673711213815~4747102206",
          "iosAppId": "ca-app-pub-8525673711213815~6303726453"
        }
      ]
    ],

    "extra": {
      "router": {},
      "eas": {
        "projectId": "aec385a6-8f71-4fe9-9dd9-e09cc3930dda"
      }
    },

    "owner": "2300090189"
  }
}
